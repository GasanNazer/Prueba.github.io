CREATE TABLE PERSON (ID BIGINT NOT NULL, CITY VARCHAR(255), FIRSTNAME VARCHAR(255), LASTNAME VARCHAR(255), PHONENUMBER VARCHAR(255), STREET VARCHAR(255), ZIPCODE VARCHAR(255), BOSS_ID BIGINT, PRIMARY KEY (ID))
CREATE TABLE EMPLOYEE (EID INTEGER NOT NULL, DEG VARCHAR(255), ENAME VARCHAR(255), SALARY DOUBLE, PRIMARY KEY (EID))
ALTER TABLE PERSON ADD CONSTRAINT FK_PERSON_BOSS_ID FOREIGN KEY (BOSS_ID) REFERENCES PERSON (ID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
